# INKSight Environment Configuration

# Required: Choose API and provide corresponded params for LLM access
USE_LOCAL_MODEL=True
OPENROUTER_API_KEY=your_openrouter_api_key_here

AGENT_LLM_MODEL=your_agent_model_here
AGENT_API_KEY=your_agent_api_key_here
AGENT_BASE_URL=your_agent_base_url_here

# Optional: LangSmith for tracing and monitoring
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=inksight

# Application Settings
APP_NAME=INKSight
APP_VERSION=1.0.0
DEBUG=false

# Server Configuration
HOST=0.0.0.0
PORT=8000

# Authentication
SECRET_KEY=your-secret-key-for-jwt-tokens
ACCESS_TOKEN_EXPIRE_MINUTES=1440